{
  "name": "ds9-local-estate",
  "version": "0.1.0",
  "description": "A local estate client for deepspace9 factorio mod",
  "main": "./build/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node --presets=env ./src --watch src",
    "build": "babel src --out-dir build --presets=env",
    "start": "node ./build start",
    "download": "node ./build download",
    "deploy": "node ./build deploy",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MasterBabol/ds9-local-estate.git"
  },
  "keywords": [
    "factorio"
  ],
  "author": "",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/MasterBabol/ds9-local-estate/issues"
  },
  "homepage": "https://github.com/MasterBabol/ds9-local-estate#readme",
  "dependencies": {
    "args": "^5.0.0",
    "babel-polyfill": "^6.26.0",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "decompress-tarxz": "^2.1.1",
    "get-port": "^4.0.0",
    "lowdb": "^1.0.0",
    "rcon-client": "^3.1.4",
    "request": "^2.88.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4",
    "babel-preset-env": "^1.7.0",
    "babel-cli": "^6.26.0"
  }
}
